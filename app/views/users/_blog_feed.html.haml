.accordion-group
  .accordion-heading
    %a.accordion-toggle{"data-parent" => "#profile-updates-widget", "data-toggle" => "collapse", :href => "#collapseFour"}
      %h4{align: 'center'}
        = "Blog feed"
  #collapseFour.accordion-body.collapse
    .accordion-inner
      %ul.media-list
        - @blog_entries.each do |entry|
          %br
          .project-name
            %h5{align: 'center'}
              = link_to entry.title, entry.url
              = entry.published
            = raw entry.summary