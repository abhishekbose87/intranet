.accordion-group
  .accordion-heading
    %a.accordion-toggle{"data-parent" => "#profile-updates-widget", "data-toggle" => "collapse", :href => "#collapseThree"}
      %h4{align: 'center'} Github profile
  #collapseThree.accordion-body.collapse.in
    .accordion-inner
      %ul.media-list
        - @github_entries.each do |entry|
          %br
          .project-name
            = raw entry.content