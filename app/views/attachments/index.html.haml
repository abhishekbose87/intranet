%div.tabbable
  %ul.nav.nav-tabs
    %li.active.document-tab
      %a{data: {toggle: 'tab'}, href: "#document"}
        Document
    %li.policy-tab
      %a{data: {toggle: "tab"}, href: "#policy"}
        Policy

  .tab-content
    .tab-pane.active#document
      = render partial: 'document'

    .tab-pane#policy
      = render partial: 'policy'
